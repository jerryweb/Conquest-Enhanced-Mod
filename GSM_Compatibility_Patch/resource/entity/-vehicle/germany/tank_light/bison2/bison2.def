	}
	{component "sticky_body"}

	{Volume "body1"
		{component "body"}
		{tags "body"}
		{thickness 15
			{front 35}
			{top 8}	
			{rear 20}
			{bottom 5}
		}
	}

	{Volume "Magazine"
		{Thickness 15}
	}
	{Volume "Left2"
		{Thickness 15}
	}
	{Volume "right2"
		{Thickness 15}
	}
	{Volume "Left1"
		{Thickness 10}
	}
	{Volume "right1"
		{Thickness 10}
	}

	{Volume "Inventory_vol"
		{Thickness 3}
	}

	{Volume "baffleBT1"
		{Thickness 20}
	}
	{Volume "baffleBT2"
		{Thickness 15}
	}
	{Volume "baffleBT3"
		{Thickness 15}
	}
	{Volume "baffleBT4"
		{Thickness 15}
	}
	{Volume "baffleBT5"
		{Thickness 15}
	}

	{Volume "BodyShieldL"
		{component "turret"}
		{tags "turret"}
		{Thickness 30}
	}
	{Volume "BodyShieldR"
		{component "turret"}
		{tags "turret"}
		{Thickness 30}
	}

	{Volume "box1"
		{Thickness 5}
	}
	{Volume "box2"
		{Thickness 35}
