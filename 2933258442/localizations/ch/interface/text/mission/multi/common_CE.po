msgid "1 waypoint"
msgstr "1 路径点"

msgctxt "mission/multi/waypoint2"
msgid "2 waypoint"
msgstr "2 路径点"

msgctxt "mission/multi/waypoint3"
msgid "3 waypoint"
msgstr "3 路径点"

msgctxt "mission/multi/waypoint4"
