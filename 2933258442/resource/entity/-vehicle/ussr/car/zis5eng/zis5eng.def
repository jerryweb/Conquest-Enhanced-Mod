{game_entity
	(include "/properties/car.ext")
	(include "/properties/selection/vehicle_elongated.inc" scale(1.15))
	(include "/properties/resupply.inc")
	{props "truck" "vision_lev02" "tractor" "tractor_heavy" "blast_large"}
	{able "emit_on_attack"}
	{extension "zis5eng.mdl"}
	{collider "truck"}
	{patherId "gazaaa"}

	{extender "enumerator"
		{digit_folder "ru1"}
	}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")

	("glass" args "window1")
	("glass" args "window2")
	("glass" args "window3")
	("glass" args "window4")
	("glass" args "window5")
	("glass" args "window6")
	("fuel" args "fuel")
	("transmission" args "transmission")

	("bodyback" repair(30))

	("shield_cover")

	{volume "body8" {able {holed}}}
	{volume "bodyback1" {material "wood"} {tags "wood"} {able {holed}}}
	{volume "bodyback2" {material "wood"} {tags "wood"}}

	{volume "shield_cover" {able {holed}}}

	("supply_engineer")

	("crew_2_human")

	("board_animation"
		place(driver)
		board_begin(board_big-midcar_driver)
		board_end(seat_driver_car)
		base(doorleft)
	)
	("board_animation"
		place(commander)
		board_begin(board_big-midcar)
		board_end(seat_passenger)
		base(doorright)
	)

	{extender "shield" {on}}

	{extender "inventory"
		{box
			{item "repair_kit" 5}

			{item "mine_detector" 2}
			{item "shovel_rus" 5}
			{item "pickaxe" 5}
			{item "satchel_charge_rus" 5}

			{item "wirecutters" 2}
		}
	}

	{mass 3100}
	{Chassis
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03d") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_car"
			speed(60) ;----------------------------------Max speed.
			speedatmaxturn(15) ;-------------------------Max speed at max turn.
			reverse(20) ;--------------------------------Max speed in Reverse.
			weight(3.1) ;------------------------------Weight in Tonnes.
			power(73) ;----------------------------------Horse Power.
			fuel(100) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(145) ;---------------------------------Range in Kilometers.
		)
	}
}