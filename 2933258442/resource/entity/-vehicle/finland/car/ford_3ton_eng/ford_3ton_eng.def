{game_entity
	(include "/properties/car.ext")
	
	(include "/properties/selection/vehicle_elongated.inc" scale(1.10))
	(include "/properties/resupply.inc")
	{props "truck" "vision_lev02" "tractor" "tractor_heavy" "blast_large"}
	{able "emit_on_attack"}
	{extension "ford_3ton_eng.mdl"}
	{collider "truck"}
	{patherId "blitz36"}

	{extender "enumerator"
		{digit_folder "ru1"}
	}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")

	("glass" args "window1")
	("glass" args "window2")
	("glass" args "window3")
	("glass" args "window4")
	("fuel" args "fuel")
	("transmission" args "transmission")

	("bodyback" repair(30))
	
	("shield_cover")

	{volume "body2" {able {holed}}}
	{volume "bodyback1" {material "wood"} {tags "wood"} {able {holed}}}
	{volume "bodyback2" {material "wood"} {tags "wood"}}
	{volume "transmission" {able {NoWoundDecal 0}}}
	
	("wood" args "bodyback1")
	("wood" args "bodyback2")
	("wood" args "bodyback3")
	("wood" args "bodyback4")
	("wood" args "bodyback5")
	("wood" args "bodyback6")
	("wood" args "bodyback7")
	
	("supply_engineer")

	("crew_2_human")

	("board_animation"
		place(driver)
		board_begin(board_big-midcar_driver)
		board_end(seat_driver_car)
		base(doorleft)
	)
	("board_animation"
		place(commander)
		board_begin(board_big-midcar)
		board_end(seat_passenger)
		base(doorright)
	)

	{extender "shield" {on}}

	{extender "inventory"
		{box
			{item "repair_kit" 5}

			{item "mine_detector" 2}
			{item "shovel_fin" 5}
			{item "pickaxe" 5}
			{item "geballteladung_3kg" 5}
			{item "wirecutters" 2}
		}
	}

	{mass 4040}
	{Chassis
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03d") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_car"
			speed(70) ;----------------------------------Max speed.
			speedatmaxturn(15) ;-------------------------Max speed at max turn.
			reverse(22) ;--------------------------------Max speed in Reverse.
			weight(4.04) ;-------------------------------Weight in Tonnes.
			power(70) ;----------------------------------Horse Power.
			fuel(110) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(230) ;---------------------------------Range in Kilometers.
		)
	}
}
